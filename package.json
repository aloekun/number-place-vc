{
  "name": "number-place-vc",
  "version": "1.0.0",
  "description": "Web-based Sudoku (Number Place) game with auto-generation and interactive solving",
  "main": "index.html",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "type-check": "echo 'No TypeScript configured - skipping type check'",
    "serve": "node server.js"
  },
  "keywords": [
    "sudoku",
    "number-place",
    "puzzle",
    "javascript",
    "web-game"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "babel-jest": "^30.0.5",
    "eslint": "^8.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.0.5"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "error",
      "no-console": "warn",
      "complexity": [
        "error",
        10
      ],
      "no-magic-numbers": [
        "error",
        {
          "ignore": [
            0,
            1,
            -1
          ]
        }
      ]
    }
  }
}
